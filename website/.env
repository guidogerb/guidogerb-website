# Copy this file to .env and fill in your values
# Vite will expose variables that start with VITE_

# Example: https://cognito-idp.us-east-1.amazonaws.com/us-east-1_XXXXXXXXX
VITE_COGNITO_AUTHORITY=

# Example: 2fd84r9p7fds882gfack2723me
VITE_COGNITO_CLIENT_ID=

# Your appâ€™s redirect/callback URL (must match Cognito app client settings)
# Defaults to window.location.origin when not provided
VITE_REDIRECT_URI=

# Usually "code" for Authorization Code Flow with PKCE
VITE_RESPONSE_TYPE=code

# Scopes to request (space-separated)
VITE_COGNITO_SCOPE="email openid phone"

# Your Cognito hosted UI domain
# Example: https://yourdomain.auth.us-east-1.amazoncognito.com
VITE_COGNITO_DOMAIN=

# Where Cognito should redirect users after logout
# Defaults to VITE_REDIRECT_URI when not provided
VITE_LOGOUT_URI=
